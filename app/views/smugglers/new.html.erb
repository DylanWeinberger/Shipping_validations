<%= form_for @smuggler, :html => { :multipart => true }   do |f| %>
<%= f.label :email %>
<%= f.email_field :email %>
<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<%= f.label :avatar %>
<%= f.file_field :image %>
<%= f.submit :create %>
<% end %>