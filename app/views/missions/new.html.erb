<h1>Mission create page.</h1>
<h1>Only Smugglers Can Create Missions</h1>
<%= form_for @mission do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :cargo %>
	<%= f.text_field :cargo, size: "60x12" %>
	<%= f.label :cost %>
	<%= f.number_field :cost, step: 0.5) %>
	<%= f.label :cost %>
	<%= f.hidden_field :smuggler_id %>

	<%= f.submit :create %>
	<% end %>



